@import url("https://fonts.googleapis.com/css2?family=DM+Sans:wght@400;500;700&family=Poppins:wght@500;600&display=swap");

:root {
  --primary-color: rgba(255, 255, 255, 0.201);
  --second-color: rgba(16, 177, 201, 0.789);
}

body {
    color: aliceblue;
    background-color: rgba(3, 3, 27, 0.914);
    font-family: sans-serif;
}

html {
    scroll-behavior: smooth;
}

.header_nav ul {
    display: flex;
    gap: 1.9em;
    justify-content: center;
    list-style: none;
    padding: .2em;
}

.header_nav a {
    text-decoration: none;
    text-transform: uppercase;
    color: aliceblue;
    padding: .6em;
    border-radius: 20em;
}

.header_nav a:hover {
    background-color:var(--primary-color);
}

.header_nav {
    background-color: rgba(27, 25, 25, 0.81);
    margin: 0 30em;
    border: solid 1px black;
    border-radius: 20em;
    margin-top: 1em ;
}


/*///////////////// aqui acaba o header e comaça o main_parte um///////////////*/

.parte_1 {
    display: flex;
    margin: 8em;
    justify-content: center;
    letter-spacing: 3px;
    line-height: 1em;
    padding: 5em 0;

}

.parte_1 img {
    width: 150px;
    height: 150px;
    border-radius: 80%;
    object-fit: cover;
    border: 3px solid var(--primary-color);
}

.inicio_color {
    padding: 0 5em ;
}

.inicio_color h1 {
    color: var(--second-color);
    text-transform: uppercase;
}

.inicio_color h2 {
    text-transform: uppercase;
}

.inicio_color p {
    color: black;
    background-color: rgba(53, 83, 236, 0.692);
    width: 21%;
    padding: .5em;
    border-radius: 10em;
    font-weight: bold;
    font-size: .9em;
}

.inicio_color p:hover {
    transform: scale(1.05);
}

.git_link a{
    text-decoration: none;
    color: white;
    display: flex;
   border: solid 1px;
   border-radius: 20em;
   padding: .4em;
    font-size: 1.2em;

}

.git_link {
    display: flex;
    gap: 1em;
}

.git_link a:hover{
    color: black;
    background-color: white;
}

/*///////////////////////////////aqui começa o a segunda parte /////*/
.sobre_mim {
    justify-content: center;
    text-align: center;
    box-sizing: border-box;
    margin: 13em 20em;
}

.sobre_mim p {
    letter-spacing: .1em;
    line-height: 1.5em;
    
}

/*///////////////////////////////aqui começa o a terceira parte /////*/

.habilidades {
    display: flex;
    margin: 15em 27em;
    gap: 2.3em;
    justify-content: center;
    flex-wrap: wrap;
}

.icones {
        background-color: rgba(27, 25, 25, 0.81);
        justify-content: center;
        text-align: center;
        padding: .1em 2em;
        border-radius: 3em 0 0;
        border-bottom: solid 3px;
}

.icones i {
    font-size: 3.9em;
    padding: .1em 0;
    color: darkblue;

}

.icones:hover {
    transform: scale(1.09);
    cursor: pointer;
}

.devicon-html5-plain, .devicon-css3-plain, .devicon-typescript-plain, .devicon-javascript-plain {
    color: darkblue;
    padding: .2em 0;

}

/*///////////////////////////////aqui começa o a quarta parte /////*/


.mae_projetos {
    display: flex;
    gap: 5em;
    max-width: 100em;
    justify-content: center;
    flex-wrap: wrap;


}

.projetos img {
    width: 30em;
}

.projetos figcaption {
    text-align: center;
    background-color: rgba(0, 0, 0, 0.605);
    margin: -.3em 0;
    padding: .5em;
}

.projetos a {
    text-decoration: none;
    color: white;
    font-weight: bold;
}

.projetos:hover {
    transform: scale(1.09);
}

/*///////////////////////////////aqui começa o a quinta parte /////*/

.contato {
    display: flex;
    padding: 20em 0;
    margin: 1em 15em;
    gap: 10em;
}
.icone-adress:hover {
    transform: scale(1.09);
    cursor: pointer;
}
.icone-adress {
    gap: 2em;
    text-align: center;
    box-sizing: border-box;

}

.icone-adress .fas {
    color: darkblue;
    background-color: black;
    font-size: 1em;
    width: 1em;
    height: 1em;
    padding: 1em;
    border-radius: 20em;
}


footer {
    background-color: rgb(2, 2, 2, .5);
    padding: 1em;
    text-align: center;
}
